import java.util.Scanner;

public class Main
{
	public static void main(String[] args) {
	    
	    //variaveis
	    
	    Scanner teclado = new Scanner(System.in);
	   
	    
	    double  C,Fr = 0;
	    
	    
	    //entradas
	    System.out.println("Conversora de celsus para fahrenheit: ");
	     System.out.println("Digite a quantidade de graus celsus: ");
	   C= teclado.nextDouble ();
	    
	 
	   
	    //processamento
	    
	    Fr = (C*1.18) + 32;
	    
	    
	    
	    
	    
	    
	    //saidas
	    
		System.out.println ("Convertido fica : "+ Fr  + " fahrenheits");
		
			
			
	   }
	
	}
