
import java.util.Scanner;

public class Main
{
	public static void main(String[] args) {
	    
	    //variaveis
	    
	    Scanner teclado = new Scanner(System.in);
	   
	    
	    double  salariomb,vr, horaaula,qntaula,acrescimo,ir,inss,salarioliq, plnodon = 0;
	    
	    
	    //entradas
	    System.out.println("Insira a sua quantidade de aulas semanais : ");
	   qntaula= teclado.nextDouble ();
	    
	   horaaula = 25.00;
	   
	    //processamento
	    
	    salariomb = (qntaula * horaaula * 4) ;
	    vr = salariomb * 0.10;
	    salariomb = salariomb + vr;
	    ir = 0.05 * salariomb ;
	    inss = 0.07 * salariomb;
	    plnodon= 0.05 * salariomb;
	    
	    salarioliq = (salariomb - ir - inss - plnodon);
	    
	    
	    
	    
	    
	    
	    //saidas
	    
		System.out.println ("Seu salario Mensal bruto e de : R$ " + salariomb);
		System.out.println ("Seu vr e de : R$ " + vr);
		System.out.println ("Seu desconto total de ir e de : R$ " + ir);
		System.out.println ("Seu desconto total de inss e de : R$ " + inss);
		System.out.println ("Seu desconto total de plano odontologico e de : R$ " + plnodon);
			System.out.println ("Seu salario liquido total e de : R$ " + salarioliq);
			
			
			
	   }
	
	}
